			<span class="o">}</span>
		<span class="o">}</span>
		<span class="kt">int</span> <span class="n">answer</span><span class="o">[]</span> <span class="o">=</span> <span class="k">new</span> <span class="kt">int</span><span class="o">[</span><span class="mi">3</span><span class="o">];</span>
		<span class="kt">int</span> <span class="n">coin</span> <span class="o">=</span> <span class="mi">10</span><span class="o">;</span>	<span class="c1">// ë³€ìˆ˜ coinìœ¼ë¡œ íšŸìˆ˜ ë³€ê²½ê°€ëŠ¥í•˜ë‹¤.</span>
		<span class="nl">END:</span>
		<span class="k">for</span><span class="o">(</span><span class="kt">int</span> <span class="n">n</span> <span class="o">=</span> <span class="mi">0</span><span class="o">;</span> <span class="n">n</span> <span class="o">&lt;</span> <span class="n">coin</span><span class="o">;</span> <span class="n">n</span><span class="o">++)</span> <span class="o">{</span>
			<span class="c1">// 2. ì‚¬ìš©ìê°€ ì…ë ¥í•œ ê°’ì„ answer ì— ê°ê° ë‹´ê¸°</span>
			<span class="nc">Scanner</span> <span class="n">input</span> <span class="o">=</span> <span class="k">new</span> <span class="nc">Scanner</span><span class="o">(</span><span class="nc">System</span><span class="o">.</span><span class="na">in</span><span class="o">);</span>
			<span class="k">for</span><span class="o">(</span><span class="kt">int</span> <span class="n">i</span> <span class="o">=</span> <span class="mi">0</span><span class="o">;</span> <span class="n">i</span> <span class="o">&lt;</span> <span class="n">answer</span><span class="o">.</span><span class="na">length</span><span class="o">;</span> <span class="n">i</span><span class="o">++)</span> <span class="o">{</span>
				<span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">printf</span><span class="o">(</span><span class="s">"%dë²ˆì§¸ ì…ë ¥í•´ì£¼ì„¸ìš” \n"</span><span class="o">,</span> <span class="n">i</span> <span class="o">+</span> <span class="mi">1</span><span class="o">);</span>
				<span class="n">answer</span><span class="o">[</span><span class="n">i</span><span class="o">]</span> <span class="o">=</span> <span class="n">input</span><span class="o">.</span><span class="na">nextInt</span><span class="o">();</span>                 <span class="c1">// answerë°°ì—´ì— ì…ë ¥í•œ ê°’ì„ ë„£ì–´ì¤€ë‹¤. </span>
			<span class="o">}</span>
			<span class="c1">// 3. baseball ë°°ì—´ê³¼ answer ë°°ì—´ì„ ê°ê° ë¹„êµ</span>
			<span class="kt">int</span> <span class="n">strike</span> <span class="o">=</span> <span class="mi">0</span><span class="o">,</span> <span class="n">ball</span> <span class="o">=</span> <span class="mi">0</span><span class="o">;</span>
			<span class="k">for</span><span class="o">(</span><span class="kt">int</span> <span class="n">i</span> <span class="o">=</span> <span class="mi">0</span><span class="o">;</span> <span class="n">i</span> <span class="o">&lt;</span> <span class="n">baseball</span><span class="o">.</span><span class="na">length</span><span class="o">;</span> <span class="n">i</span><span class="o">++)</span> <span class="o">{</span>
				<span class="k">for</span><span class="o">(</span><span class="kt">int</span> <span class="n">j</span> <span class="o">=</span> <span class="mi">0</span><span class="o">;</span> <span class="n">j</span> <span class="o">&lt;</span> <span class="n">answer</span><span class="o">.</span><span class="na">length</span><span class="o">;</span> <span class="n">j</span><span class="o">++)</span> <span class="o">{</span>
					<span class="k">if</span><span class="o">(</span><span class="n">baseball</span><span class="o">[</span><span class="n">i</span><span class="o">]</span> <span class="o">==</span> <span class="n">answer</span><span class="o">[</span><span class="n">j</span><span class="o">]</span> <span class="o">&amp;&amp;</span> <span class="n">i</span> <span class="o">==</span> <span class="n">j</span><span class="o">)</span> <span class="o">{</span>        <span class="c1">// baseballë°°ì—´ê³¼ answerë°°ì—´ì˜ ê°’ì´ ê°™ê³  ìœ„ì¹˜ê°€ ê°™ìœ¼ë©´ strike += 1;</span>
						<span class="n">strike</span> <span class="o">+=</span> <span class="mi">1</span><span class="o">;</span>
					<span class="o">}</span><span class="k">else</span> <span class="k">if</span><span class="o">(</span><span class="n">baseball</span><span class="o">[</span><span class="n">i</span><span class="o">]</span> <span class="o">==</span> <span class="n">answer</span><span class="o">[</span><span class="n">j</span><span class="o">]</span> <span class="o">&amp;&amp;</span> <span class="n">i</span> <span class="o">!=</span> <span class="n">j</span><span class="o">)</span> <span class="o">{</span>  <span class="c1">// baseballë°°ì—´ê³¼ answerë°°ì—´ì˜ ê°’ì´ ê°™ê³  ìœ„ì¹˜ê°€ ë‹¤ë¥´ë©´ ball += 1;</span>
						<span class="n">ball</span> <span class="o">+=</span> <span class="mi">1</span><span class="o">;</span>
					<span class="o">}</span>
				<span class="o">}</span>
			<span class="o">}</span>
			<span class="k">if</span><span class="o">(</span><span class="n">strike</span> <span class="o">==</span> <span class="mi">3</span><span class="o">)</span> <span class="o">{</span>
				<span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">printf</span><span class="o">(</span><span class="s">"%dìŠ¤íŠ¸ë¼ì´í¬ìœ¼ë¡œ ì´ë‹ì´ ì¢…ë£Œë˜ì—ˆìŠµë‹ˆë‹¤ \n"</span><span class="o">,</span> <span class="n">strike</span><span class="o">);</span>
				<span class="k">break</span> <span class="no">END</span><span class="o">;</span>                                                   <span class="c1">// 3ìŠ¤íŠ¸ë¼ì´í¬ê°€ ë‚˜ì˜¤ë©´ break END; í•´ì¤€ë‹¤. </span>
			<span class="o">}</span><span class="k">else</span> <span class="o">{</span>
				<span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">printf</span><span class="o">(</span><span class="s">"%dë³¼, %dìŠ¤íŠ¸ë¼ì´í¬ì…ë‹ˆë‹¤ \n"</span><span class="o">,</span> <span class="n">ball</span><span class="o">,</span> <span class="n">strike</span><span class="o">);</span>   <span class="c1">// ê·¸ ì™¸ ê²°ê³¼ë¥¼ ì¶œë ¥í•´ì¤€ë‹¤. </span>
			<span class="o">}</span>
			<span class="k">if</span><span class="o">(</span><span class="n">n</span> <span class="o">==</span> <span class="n">coin</span> <span class="o">-</span> <span class="mi">1</span><span class="o">)</span> <span class="o">{</span>
				<span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="s">"ì‹œë„íšŸìˆ˜ê°€ ì´ˆê³¼ë¬ìŠµë‹ˆë‹¤"</span><span class="o">);</span>                      <span class="c1">// ì§€ì •í•œ coinì„ ë‹¤ì“¸ ì‹œ ì‹œë„íšŸìˆ˜ ì´ˆê³¼ë¥¼ ì•Œë ¤ì¤€ë‹¤.</span>
			<span class="o">}</span>
		<span class="o">}</span>
		<span class="c1">// [ë¬¸ì œ] ë°© ë²ˆí˜¸</span>
		<span class="c1">// ë°© ë²ˆí˜¸ê°€ ì£¼ì–´ì¡Œì„ëŒ€ í•„ìš”í•œ ì„¸íŠ¸ì˜ ê°œìˆ˜ì˜ ìµœì†Ÿê°’</span>
		<span class="c1">// ë°©ë²ˆí˜¸ ì…ë ¥</span>
		<span class="nc">Scanner</span> <span class="n">input</span> <span class="o">=</span> <span class="k">new</span> <span class="nc">Scanner</span><span class="o">(</span><span class="nc">System</span><span class="o">.</span><span class="na">in</span><span class="o">);</span>
		<span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="s">"ë°© ë²ˆí˜¸ë¥¼ ì…ë ¥í•˜ì„¸ìš” (ex)0~1,000,000ê¹Œì§€ ì„¤ì •í•  ìˆ˜ ìˆìŠµë‹ˆë‹¤"</span><span class="o">);</span>
		<span class="nc">String</span> <span class="n">room_num</span> <span class="o">=</span> <span class="n">input</span><span class="o">.</span><span class="na">next</span><span class="o">();</span>		
		<span class="c1">// ë°© ë²ˆí˜¸ ë°°ì—´</span>
		<span class="kt">char</span><span class="o">[]</span> <span class="n">num</span> <span class="o">=</span> <span class="k">new</span> <span class="kt">char</span><span class="o">[</span><span class="n">room_num</span><span class="o">.</span><span class="na">length</span><span class="o">()];</span> <span class="c1">// ì…ë ¥í•œ ë°©ë²ˆí˜¸ë¥¼ ë¬¸ìë¡œ ë°”ê¿”ì¤€ë‹¤.</span>
		<span class="kt">int</span> <span class="o">[]</span> <span class="n">num_int</span> <span class="o">=</span> <span class="k">new</span> <span class="kt">int</span><span class="o">[</span><span class="n">num</span><span class="o">.</span><span class="na">length</span><span class="o">];</span> <span class="c1">// ë¬¸ìë¡œ ë°›ì€ ë°© ë²ˆí˜¸ì˜ ê¸¸ì´ë¥¼ ìˆ˜ë¡œ ë°”ê¿”ì¤€ë‹¤.		</span>
		<span class="kt">int</span> <span class="o">[]</span> <span class="n">sum</span> <span class="o">=</span> <span class="k">new</span> <span class="kt">int</span><span class="o">[</span><span class="mi">10</span><span class="o">];</span>
		<span class="c1">// ë°© ë²ˆí˜¸ì˜ ìë¦¬ìˆ˜ë§Œí¼ ë°˜ë³µí•´ì„œ ë“¤ì–´ê°€ëŠ” ìˆ«ìì˜ ì´í•©ì„ êµ¬í•œë‹¤.</span>
		<span class="k">for</span><span class="o">(</span><span class="kt">int</span> <span class="n">i</span> <span class="o">=</span> <span class="mi">0</span><span class="o">;</span> <span class="n">i</span> <span class="o">&lt;</span> <span class="n">num_int</span><span class="o">.</span><span class="na">length</span><span class="o">;</span> <span class="n">i</span><span class="o">++)</span> <span class="o">{</span>
			<span class="n">num</span><span class="o">[</span><span class="n">i</span><span class="o">]</span> <span class="o">=</span> <span class="n">room_num</span><span class="o">.</span><span class="na">charAt</span><span class="o">(</span><span class="n">i</span><span class="o">);</span> <span class="c1">// ë°©ë²ˆí˜¸ë¥¼ ì˜ë¼ì¤€ë‹¤.</span>
