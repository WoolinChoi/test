					<span class="n">sum</span><span class="o">[</span><span class="n">i</span><span class="o">]++;</span>
				<span class="o">}</span><span class="k">else</span> <span class="k">if</span><span class="o">(</span><span class="n">j</span> <span class="o">==</span> <span class="mi">9</span><span class="o">)</span> <span class="o">{</span>
