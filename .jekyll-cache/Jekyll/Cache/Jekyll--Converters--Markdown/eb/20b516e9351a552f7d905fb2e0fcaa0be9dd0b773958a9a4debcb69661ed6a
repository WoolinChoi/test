			<span class="o">}</span><span class="k">else</span> <span class="o">{</span>
				<span class="n">sess</span><span class="o">.</span><span class="na">rollback</span><span class="o">();</span>
			<span class="o">}</span>
		<span class="o">}</span><span class="k">finally</span> <span class="o">{</span>
			<span class="n">sess</span><span class="o">.</span><span class="na">close</span><span class="o">();</span>
		<span class="o">}</span>
	<span class="o">}</span>
	
	<span class="c1">// ê¸€ ë²ˆí˜¸ë¡œ ë°©ëª…ë¡ ì‚­ì œ</span>
	<span class="kd">public</span> <span class="kt">int</span> <span class="nf">deleteCommentByPK</span><span class="o">(</span><span class="kt">long</span> <span class="n">commentNo</span><span class="o">)</span> <span class="o">{</span>
		<span class="nc">SqlSession</span> <span class="n">sess</span> <span class="o">=</span> <span class="n">getSqlSessionFactory</span><span class="o">().</span><span class="na">openSession</span><span class="o">();</span>
