<h2 id="commentmapper_xml">CommentMapper_xml</h2>
<div class="language-xml highlighter-rouge"><div class="highlight"><pre class="highlight"><code><span class="cp">&lt;?xml version="1.0" encoding="UTF-8" ?&gt;</span>
<span class="cp">&lt;!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd"&gt;</span>
<span class="nt">&lt;mapper</span> <span class="na">namespace=</span><span class="s">"mapper.CommentMapper"</span><span class="nt">&gt;</span>
	<span class="nt">&lt;select</span> <span class="na">id=</span><span class="s">"selectComment"</span> <span class="na">parameterType=</span><span class="s">"hashmap"</span> <span class="na">resultType=</span><span class="s">"Comment"</span><span class="nt">&gt;</span>
		SELECT * 
		FROM comment_tab
		<span class="nt">&lt;where&gt;</span>
			<span class="c">&lt;!-- &lt;![CDATA[comment_no &gt;= 20]]&gt; --&gt;</span>
			<span class="nt">&lt;if</span> <span class="na">test=</span><span class="s">"commentNo != null"</span><span class="nt">&gt;</span>
				comment_no=#{commentNo}
			<span class="nt">&lt;/if&gt;</span>
		<span class="nt">&lt;/where&gt;</span>
	<span class="nt">&lt;/select&gt;</span>
	
<span class="c">&lt;!-- 	&lt;select id="selectCommentByPK" parameterType="long" resultType="Comment"&gt; --&gt;</span>
<span class="c">&lt;!-- 		SELECT *  --&gt;</span>
<span class="c">&lt;!-- 		FROM comment_tab  --&gt;</span>
<span class="c">&lt;!-- 		WHERE comment_no=#{commentNo} --&gt;</span>
<span class="c">&lt;!-- 	&lt;/select&gt; --&gt;</span>
	
	<span class="nt">&lt;insert</span> <span class="na">id=</span><span class="s">"insertComment"</span> <span class="na">parameterType=</span><span class="s">"Comment"</span><span class="nt">&gt;</span>
		INSERT INTO comment_tab(comment_no, user_id, comment_content, reg_date) 
		VALUES (seq_comment_no.nextval, #{userId}, #{commentContent}, sysdate)
	<span class="nt">&lt;/insert&gt;</span>
	
	<span class="nt">&lt;delete</span> <span class="na">id=</span><span class="s">"deleteCommentByPK"</span> <span class="na">parameterType=</span><span class="s">"long"</span><span class="nt">&gt;</span>
		DELETE FROM comment_tab 
		WHERE comment_no=#{commentNo}
	<span class="nt">&lt;/delete&gt;</span>
	
	<span class="nt">&lt;update</span> <span class="na">id=</span><span class="s">"modifyCommentByPK"</span> <span class="na">parameterType=</span><span class="s">"long"</span><span class="nt">&gt;</span>
		UPDATE comment_tab 
		SET comment_content=#{commentContent}
		WHERE comment_no=#{commentNo}
	<span class="nt">&lt;/update&gt;</span>
<span class="nt">&lt;/mapper&gt;</span>
</code></pre></div></div>

<h2 id="commentrepository">CommentRepository</h2>
<div class="language-java highlighter-rouge"><div class="highlight"><pre class="highlight"><code><span class="kn">package</span> <span class="nn">mybatis.guest.session</span><span class="o">;</span>
<span class="kn">import</span> <span class="nn">java.io.InputStream</span><span class="o">;</span>
<span class="kn">import</span> <span class="nn">java.util.*</span><span class="o">;</span>
<span class="kn">import</span> <span class="nn">org.apache.ibatis.io.Resources</span><span class="o">;</span>
