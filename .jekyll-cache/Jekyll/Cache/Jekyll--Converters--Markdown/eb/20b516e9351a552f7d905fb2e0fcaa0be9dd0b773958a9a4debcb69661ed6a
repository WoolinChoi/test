			<span class="kt">int</span> <span class="n">result</span> <span class="o">=</span> <span class="n">sess</span><span class="o">.</span><span class="na">delete</span><span class="o">(</span><span class="n">namespace</span> <span class="o">+</span> <span class="s">".deleteCommentByPK"</span><span class="o">,</span> <span class="n">commentNo</span><span class="o">);</span>
