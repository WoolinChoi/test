		<span class="o">}</span><span class="k">finally</span> <span class="o">{</span>
			<span class="n">sess</span><span class="o">.</span><span class="na">close</span><span class="o">();</span>
		<span class="o">}</span>
	<span class="o">}</span>
	
	<span class="c1">// ë°©ëª…ë¡ ì…ë ¥</span>
	<span class="kd">public</span> <span class="kt">void</span> <span class="nf">insertComment</span><span class="o">(</span><span class="nc">Comment</span> <span class="n">c</span><span class="o">)</span> <span class="o">{</span>
