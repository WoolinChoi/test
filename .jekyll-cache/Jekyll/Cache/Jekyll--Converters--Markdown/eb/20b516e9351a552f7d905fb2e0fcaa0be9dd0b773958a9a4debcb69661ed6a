		<span class="o">}</span><span class="k">finally</span> <span class="o">{</span>
			<span class="n">sess</span><span class="o">.</span><span class="na">close</span><span class="o">();</span>
		<span class="o">}</span>
	<span class="o">}</span>
<span class="o">}</span>
</code></pre></div></div>

<h2 id="commentservice">CommentService</h2>
<div class="language-java highlighter-rouge"><div class="highlight"><pre class="highlight"><code><span class="kn">package</span> <span class="nn">mybatis.guest.service</span><span class="o">;</span>
<span class="kn">import</span> <span class="nn">java.util.List</span><span class="o">;</span>
<span class="kn">import</span> <span class="nn">mybatis.guest.model.Comment</span><span class="o">;</span>
<span class="kn">import</span> <span class="nn">mybatis.guest.session.CommentRepository</span><span class="o">;</span>
<span class="kd">public</span> <span class="kd">class</span> <span class="nc">CommentService</span> <span class="o">{</span>
	<span class="kd">private</span> <span class="kd">static</span> <span class="nc">CommentService</span> <span class="n">service</span><span class="o">;</span>
	
	<span class="kd">private</span> <span class="nf">CommentService</span><span class="o">()</span> <span class="o">{}</span>
	
	<span class="kd">public</span> <span class="kd">static</span> <span class="nc">CommentService</span> <span class="nf">getInstance</span><span class="o">()</span> <span class="o">{</span>
		<span class="k">if</span><span class="o">(</span><span class="n">service</span> <span class="o">==</span> <span class="kc">null</span><span class="o">)</span> <span class="o">{</span>
			<span class="n">service</span> <span class="o">=</span> <span class="k">new</span> <span class="nc">CommentService</span><span class="o">();</span>
		<span class="o">}</span>
		<span class="k">return</span> <span class="n">service</span><span class="o">;</span>
	<span class="o">}</span>
	
	<span class="nc">CommentRepository</span> <span class="n">repo</span> <span class="o">=</span> <span class="k">new</span> <span class="nc">CommentRepository</span><span class="o">();</span>
	<span class="c1">// ë°©ëª…ë¡ ì „ì²´ ê²€ìƒ‰</span>
	<span class="kd">public</span> <span class="nc">List</span><span class="o">&lt;</span><span class="nc">Comment</span><span class="o">&gt;</span> <span class="nf">selectComment</span><span class="o">(){</span>
		<span class="k">return</span> <span class="n">repo</span><span class="o">.</span><span class="na">selectComment</span><span class="o">();</span>
	<span class="o">}</span>
	
	<span class="c1">// ë°©ëª…ë¡ ê¸€ ë²ˆí˜¸ë¡œ ê²€ìƒ‰(return ê°’ì˜ ìë£Œí˜•ì€ Comment)</span>
	<span class="kd">public</span> <span class="nc">Comment</span> <span class="nf">selectCommentByPrimaryKey</span><span class="o">(</span><span class="kt">long</span> <span class="n">cId</span><span class="o">)</span> <span class="o">{</span>
		<span class="k">return</span> <span class="n">repo</span><span class="o">.</span><span class="na">selectCommentByPK</span><span class="o">(</span><span class="n">cId</span><span class="o">);</span>
	<span class="o">}</span>
	
	<span class="c1">// ë°©ëª…ë¡ ì…ë ¥(returnê°’ì´ í•„ìš”ì—†ê¸° ë•Œë¬¸ì— void)</span>
	<span class="kd">public</span> <span class="kt">void</span> <span class="nf">insertComment</span><span class="o">(</span><span class="nc">Comment</span> <span class="n">c</span><span class="o">)</span> <span class="o">{</span>
		<span class="n">repo</span><span class="o">.</span><span class="na">insertComment</span><span class="o">(</span><span class="n">c</span><span class="o">);</span>
	<span class="o">}</span>
	
	<span class="c1">// ê¸€ ë²ˆí˜¸ë¡œ ë°©ëª…ë¡ ì‚­ì œ(return ê°’ì˜ ìë£Œí˜•ì€ int)</span>
	<span class="kd">public</span> <span class="kt">int</span> <span class="nf">deleteCommentByPrimaryKey</span><span class="o">(</span><span class="kt">long</span> <span class="n">cId</span><span class="o">)</span> <span class="o">{</span>
		<span class="k">return</span> <span class="n">repo</span><span class="o">.</span><span class="na">deleteCommentByPK</span><span class="o">(</span><span class="n">cId</span><span class="o">);</span>
	<span class="o">}</span>
	
	<span class="c1">// ê¸€ ë²ˆí˜¸ë¡œ ë°©ëª…ë¡ ìˆ˜ì •(return ê°’ì˜ ìë£Œí˜•ì€ int)</span>
	<span class="kd">public</span> <span class="kt">int</span> <span class="nf">modifyCommentByPrimaryKey</span><span class="o">(</span><span class="nc">Comment</span> <span class="n">c</span><span class="o">){</span>
		<span class="k">return</span> <span class="n">repo</span><span class="o">.</span><span class="na">modifyCommentByPK</span><span class="o">(</span><span class="n">c</span><span class="o">);</span>
	<span class="o">}</span>
<span class="o">}</span>
