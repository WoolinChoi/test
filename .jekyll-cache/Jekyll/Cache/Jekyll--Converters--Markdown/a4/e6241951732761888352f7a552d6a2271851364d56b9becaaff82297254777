	<span class="nt">&lt;form</span> <span class="na">name=</span><span class="s">"frm"</span> <span class="na">action=</span><span class="s">"00_server.jsp"</span><span class="nt">&gt;</span>
	<span class="nt">&lt;input</span> <span class="na">type=</span><span class="s">"submit"</span> <span class="na">value=</span><span class="s">"server.jspíŒŒì¼ì„ ë¶ˆëŸ¬ ë°ì´í„° ë°›ì•„ì˜´"</span><span class="nt">&gt;</span>
	<span class="nt">&lt;br/&gt;</span>
	ì„œë²„ë¡œë¶€í„° ë„˜ê²¨ë°›ì€ ë°ì´í„°<span class="nt">&lt;br/&gt;</span>
	ì²«ë²ˆì§¸ ë°ì´í„° : <span class="nt">&lt;input</span> <span class="na">type=</span><span class="s">"text"</span> <span class="na">name=</span><span class="s">"cate"</span> <span class="na">id=</span><span class="s">"cate"</span> <span class="na">value=</span><span class="s">"</span><span class="nt">&lt;%=</span> <span class="n">cate</span> <span class="nt">%&gt;</span><span class="s">"</span><span class="nt">/&gt;&lt;br/&gt;</span>
	ë‘ë²ˆì§¸ ë°ì´í„° : <span class="nt">&lt;input</span> <span class="na">type=</span><span class="s">"text"</span> <span class="na">name=</span><span class="s">"name"</span> <span class="na">id=</span><span class="s">"name"</span> <span class="na">value=</span><span class="s">"</span><span class="nt">&lt;%=</span> <span class="n">name</span> <span class="nt">%&gt;</span><span class="s">"</span><span class="nt">/&gt;&lt;br/&gt;</span>
