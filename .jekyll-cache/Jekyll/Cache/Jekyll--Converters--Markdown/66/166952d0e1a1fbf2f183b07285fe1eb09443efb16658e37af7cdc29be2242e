	<span class="err">$</span><span class="o">(</span><span class="err">'#</span><span class="n">total_btn</span><span class="o">&gt;</span><span class="n">a</span><span class="err">'</span><span class="o">).</span><span class="na">click</span><span class="o">(</span><span class="n">function</span><span class="o">(){</span>
