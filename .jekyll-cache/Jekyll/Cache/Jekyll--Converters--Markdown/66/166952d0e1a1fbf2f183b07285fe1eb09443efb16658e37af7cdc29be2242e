		<span class="n">buttonImg</span><span class="o">.</span><span class="na">attr</span><span class="o">(</span><span class="err">'</span><span class="n">src</span><span class="err">'</span><span class="o">,</span> <span class="n">buttonImg</span><span class="o">.</span><span class="na">attr</span><span class="o">(</span><span class="err">'</span><span class="n">src</span><span class="err">'</span><span class="o">).</span><span class="na">replace</span><span class="o">(</span><span class="err">'</span><span class="n">_out</span><span class="err">'</span><span class="o">,</span> <span class="err">'</span><span class="n">_over</span><span class="err">'</span><span class="o">));</span>
		<span class="c1">// ëˆ„ë¥¸ buttonì˜ _outì„ _overë¡œ ë³€ê²½</span>
		<span class="n">lastButtonImg</span><span class="o">.</span><span class="na">find</span><span class="o">(</span><span class="err">'</span><span class="n">img</span><span class="err">'</span><span class="o">).</span><span class="na">attr</span><span class="o">(</span><span class="err">'</span><span class="n">src</span><span class="err">'</span><span class="o">,</span> <span class="n">lastButtonImg</span><span class="o">.</span><span class="na">find</span><span class="o">(</span><span class="err">'</span><span class="n">img</span><span class="err">'</span><span class="o">).</span><span class="na">attr</span><span class="o">(</span><span class="err">'</span><span class="n">src</span><span class="err">'</span><span class="o">).</span><span class="na">replace</span><span class="o">(</span><span class="err">'</span><span class="n">_over</span><span class="err">'</span><span class="o">,</span> <span class="err">'</span><span class="n">_out</span><span class="err">'</span><span class="o">));</span>
