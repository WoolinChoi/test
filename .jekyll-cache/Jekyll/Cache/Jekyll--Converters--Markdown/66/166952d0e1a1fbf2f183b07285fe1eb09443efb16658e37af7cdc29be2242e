		<span class="kt">var</span> <span class="n">buttonImg</span> <span class="o">=</span> <span class="err">$</span><span class="o">(</span><span class="k">this</span><span class="o">).</span><span class="na">find</span><span class="o">(</span><span class="err">'</span><span class="n">img</span><span class="err">'</span><span class="o">);</span>
