	<span class="kt">var</span> <span class="n">popBanner</span> <span class="o">=</span> <span class="err">$</span><span class="o">(</span><span class="err">'#</span><span class="n">roll_banner_wrap</span><span class="o">&gt;</span><span class="n">dl</span><span class="err">'</span><span class="o">).</span><span class="na">bxSlider</span><span class="o">({</span>
