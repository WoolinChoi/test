       <span class="n">auto</span> <span class="o">:</span> <span class="kc">true</span><span class="o">,</span>    <span class="c1">// ìë™ ì‹¤í–‰ ì—¬ë¶€</span>
       <span class="n">speed</span> <span class="o">:</span> <span class="mi">500</span><span class="o">,</span>    <span class="c1">// ì´ë™ ì†ë„ ì„¤ì •</span>
       <span class="n">moveSlides</span> <span class="o">:</span> <span class="mi">1</span><span class="o">,</span>    <span class="c1">// ìŠ¬ë¼ì´ë“œ ì´ë™ ê°¯ìˆ˜</span>
       <span class="nl">pager:</span> <span class="kc">false</span>    <span class="c1">// í˜„ì¬ ìœ„ì¹˜ í˜ì´ì§• í‘œì‹œ ì—¬ë¶€ ì„¤ì •</span>
    <span class="o">});</span>
    <span class="c1">// ì´ì „ë²„íŠ¼ì„ í´ë¦­í•˜ë©´ ì´ì „ìŠ¬ë¼ì´ë“œë¡œ ì „í™˜</span>
    <span class="err">$</span><span class="o">(</span><span class="err">'</span><span class="o">.</span><span class="na">prev_btn</span><span class="err">'</span><span class="o">).</span><span class="na">on</span><span class="o">(</span><span class="err">'</span><span class="n">click</span><span class="err">'</span><span class="o">,</span> <span class="n">function</span><span class="o">(){</span>
       <span class="n">slider</span><span class="o">.</span><span class="na">goToPrevSlide</span><span class="o">();</span>    <span class="c1">// ì´ì „ ìŠ¬ë¼ì´ë“œ ë°°ë„ˆë¡œ ì´ë™</span>
    <span class="o">});</span>
    <span class="c1">// ë‹¤ìŒë²„íŠ¼ì„ í´ë¦­í•˜ë©´ ë‹¤ìŒìŠ¬ë¼ì´ë“œë¡œ ì „í™˜</span>
    <span class="err">$</span><span class="o">(</span><span class="err">'</span><span class="o">.</span><span class="na">next_btn</span><span class="err">'</span><span class="o">).</span><span class="na">on</span><span class="o">(</span><span class="err">'</span><span class="n">click</span><span class="err">'</span><span class="o">,</span> <span class="n">function</span><span class="o">(){</span>
       <span class="n">slider</span><span class="o">.</span><span class="na">goToNextSlide</span><span class="o">();</span>    <span class="c1">// ë‹¤ìŒ ìŠ¬ë¼ì´ë“œ ë°°ë„ˆë¡œ ì´ë™</span>
    <span class="o">});</span>
    <span class="c1">// ìŠ¬ë¼ì´ë” ì™¼ìª½ì— ê³µê°„ì„ ì¤Œ</span>
    <span class="n">slider</span><span class="o">.</span><span class="na">css</span><span class="o">(</span> <span class="err">'</span><span class="n">margin</span><span class="o">-</span><span class="n">left</span><span class="err">'</span><span class="o">,</span> <span class="err">'</span><span class="mi">10</span><span class="n">px</span><span class="err">'</span> <span class="o">);</span>
    
	<span class="c1">// ########[5] ë¡œê·¸ì¸ ########</span>
	<span class="c1">// ë¡œê·¸ì¸  í´ë¦­</span>
	<span class="err">$</span><span class="o">(</span><span class="err">'</span><span class="o">.</span><span class="na">login_wrap</span><span class="o">&gt;</span><span class="n">a</span><span class="err">'</span><span class="o">).</span><span class="na">click</span><span class="o">(</span><span class="n">function</span><span class="o">(){</span>
		<span class="c1">// í¼ì˜ animate() ì´ìš©í•˜ì—¬ top ìœ„ì¹˜ë¥¼ 20pxë¡œ ì§€ì •</span>
		<span class="err">$</span><span class="o">(</span><span class="err">'#</span><span class="n">login_f</span><span class="err">'</span><span class="o">).</span><span class="na">animate</span><span class="o">({</span>
			<span class="err">'</span><span class="n">top</span><span class="err">'</span> <span class="o">:</span> <span class="err">'</span><span class="mi">20</span><span class="n">px</span><span class="err">'</span>
		<span class="o">});</span>
	<span class="o">});</span>
	<span class="c1">// í¼ì˜ ë‹«ê¸° í´ë¦­ </span>
	<span class="err">$</span><span class="o">(</span><span class="err">'</span><span class="o">.</span><span class="na">login_close_btn</span><span class="o">&gt;</span><span class="n">a</span><span class="err">'</span><span class="o">).</span><span class="na">click</span><span class="o">(</span><span class="n">function</span><span class="o">(){</span>
		<span class="c1">// í¼ì˜ animate() ì´ìš©í•˜ì—¬ top ìœ„ì¹˜ë¥¼ -500pxë¡œ ì§€ì •</span>
		<span class="err">$</span><span class="o">(</span><span class="err">'#</span><span class="n">login_f</span><span class="err">'</span><span class="o">).</span><span class="na">animate</span><span class="o">({</span>
