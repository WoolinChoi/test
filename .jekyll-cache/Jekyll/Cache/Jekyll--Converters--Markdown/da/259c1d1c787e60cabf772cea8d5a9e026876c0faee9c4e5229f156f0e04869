<h2 id="scanner">Scanner</h2>
<div class="language-java highlighter-rouge"><div class="highlight"><pre class="highlight"><code><span class="kn">import</span> <span class="nn">java.util.Scanner</span><span class="o">;</span>
<span class="c1">// import java.lang.*;	// *ë¡œ ëª¨ë‘ import í•´ì¤€ë‹¤.</span>
<span class="c1">// ctrl + shift + o</span>
<span class="kd">public</span> <span class="kd">class</span> <span class="nc">Ex05_Scanner</span> <span class="o">{</span>
	<span class="kd">public</span> <span class="kd">static</span> <span class="kt">void</span> <span class="nf">main</span><span class="o">(</span><span class="nc">String</span><span class="o">[]</span> <span class="n">args</span><span class="o">)</span> <span class="o">{</span>
		<span class="nc">Scanner</span> <span class="n">input</span> <span class="o">=</span> <span class="k">new</span> <span class="nc">Scanner</span><span class="o">(</span><span class="nc">System</span><span class="o">.</span><span class="na">in</span><span class="o">);</span>
		<span class="c1">// í•™ìƒëª…ì„ ì…ë ¥ ë°›ì•„</span>
		<span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="s">"í•™ìƒì´ë¦„ì…ë ¥=&gt;"</span><span class="o">);</span>
		<span class="nc">String</span> <span class="n">name</span> <span class="o">=</span> <span class="n">input</span><span class="o">.</span><span class="na">nextLine</span><span class="o">();</span>	
		<span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">print</span><span class="o">(</span><span class="s">"êµ­ì–´ì ìˆ˜=&gt;"</span><span class="o">);</span>
		<span class="kt">int</span> <span class="n">kor</span> <span class="o">=</span> <span class="n">input</span><span class="o">.</span><span class="na">nextInt</span><span class="o">();</span>
		<span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">print</span><span class="o">(</span><span class="s">"ì˜ì–´ì ìˆ˜=&gt;"</span><span class="o">);</span>
		<span class="kt">int</span> <span class="n">eng</span> <span class="o">=</span> <span class="n">input</span><span class="o">.</span><span class="na">nextInt</span><span class="o">();</span>
		<span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">print</span><span class="o">(</span><span class="s">"ìˆ˜í•™ì ìˆ˜=&gt;"</span><span class="o">);</span>
		<span class="kt">int</span> <span class="n">math</span> <span class="o">=</span> <span class="n">input</span><span class="o">.</span><span class="na">nextInt</span><span class="o">();</span>
		<span class="c1">// ì´ì ì„ êµ¬í•´ì„œ ì¶œë ¥</span>
		<span class="kt">int</span> <span class="n">total</span> <span class="o">=</span> <span class="n">kor</span> <span class="o">+</span> <span class="n">eng</span> <span class="o">+</span> <span class="n">math</span><span class="o">;</span>
		<span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="s">"ì´ì : "</span> <span class="o">+</span> <span class="n">total</span><span class="o">);</span>
		<span class="c1">// í‰ê· ì„ êµ¬í•´ì„œ ì¶œë ¥</span>
		<span class="kt">double</span> <span class="n">avg</span> <span class="o">=</span> <span class="o">(</span><span class="kt">double</span><span class="o">)</span><span class="n">total</span><span class="o">/</span><span class="mi">3</span><span class="o">;</span>	<span class="c1">// intí˜•ê³¼ intí˜•ì„ ê³„ì‚°í•˜ë©´ intí˜•ìœ¼ë¡œ ê³„ì‚°ë˜ë‹ˆ doubleí˜•ìœ¼ë¡œ castingí•´ì¤€ë‹¤.</span>
		<span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="s">"í‰ê· : "</span> <span class="o">+</span> <span class="n">avg</span><span class="o">);</span>
	<span class="o">}</span>
