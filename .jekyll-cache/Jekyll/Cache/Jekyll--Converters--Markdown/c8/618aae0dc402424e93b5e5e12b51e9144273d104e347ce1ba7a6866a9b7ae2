    <span class="nt">&lt;script&gt;</span>
      <span class="nx">$</span><span class="p">(</span><span class="dl">'</span><span class="s1">.carousel</span><span class="dl">'</span><span class="p">).</span><span class="nx">carousel</span><span class="p">({</span>
    	  <span class="na">interval</span><span class="p">:</span><span class="mi">1000</span>
