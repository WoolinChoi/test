<span class="c1">// 		$('li').not(':first').css('background', 'red');</span>
<span class="c1">// 		$('li:not(li:first)').css('background', 'red');</span>
		
		<span class="c1">// (6) 'íŠ€ê¹€'ì„ í¬í•¨í•œ ìš”ì†Œ</span>
<span class="c1">// 		$('li').filter(':contains("íŠ€ê¹€")').css('background', 'red');</span>
