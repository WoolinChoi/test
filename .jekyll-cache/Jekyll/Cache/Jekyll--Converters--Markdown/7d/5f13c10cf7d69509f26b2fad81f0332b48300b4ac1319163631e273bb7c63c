<span class="c1">// 		$('li:eq(3)').css('background', 'red');</span>
		
		<span class="c1">// (4) ì„¸ë²ˆì§¸ê¹Œì§€ì˜ ìš”ì†Œ</span>
<span class="c1">// 		$('li').filter(':lt(2)').css('background', 'red');</span>
<span class="c1">// 		$('li:lt(2)').css('background', 'red');</span>
		
