<span class="c1">// 			$(this).click(function(){</span>
<span class="c1">// 				$(this).toggleClass('bg');</span>
<span class="c1">// 			});</span>
<span class="c1">// 		});</span>
		
		<span class="c1">// (1) ì–´ë¬µ ë°‘ì— ìì‹ ìš”ì†Œë“¤ë§Œ cssì˜ border í´ë˜ìŠ¤ ì ìš©</span>
<span class="c1">// 		$('li:contains("ì–´ë¬µ")').find('li:parent').css('border', '3px dotted red');</span>
<span class="c1">// 		$('li').filter(function(index){ return (index &gt; 2) &amp; (index </span><span class="o">&lt;</span> <span class="mi">6</span><span class="p">)</span> <span class="p">}).</span><span class="nx">css</span><span class="p">(</span><span class="dl">'</span><span class="s1">border</span><span class="dl">'</span><span class="p">,</span> <span class="dl">'</span><span class="s1">3px dotted red</span><span class="dl">'</span><span class="p">);</span>

		<span class="c1">// (2) li ìš”ì†Œ ë§ˆìš°ìŠ¤ê°€ ì˜¬ë¼ê°€ë©´ bg í´ë˜ìŠ¤ ì ìš©, ë§ˆìš°ìŠ¤ê°€ ë‚´ë ¤ê°€ë©´ bg í´ë˜ìŠ¤ê°€ ì œê±°í•œë‹¤. ë‹¨, ì–´ë¬µ ë°‘ì— ìì‹ìš”ì†ŒëŠ” ì œì™¸</span>
		<span class="nx">$</span><span class="p">(</span><span class="dl">'</span><span class="s1">li</span><span class="dl">'</span><span class="p">).</span><span class="nx">not</span><span class="p">(</span><span class="dl">'</span><span class="s1">#food ul &gt; li, #food :nth-child(3)</span><span class="dl">'</span><span class="p">).</span><span class="nx">hover</span><span class="p">(</span>
			<span class="kd">function</span><span class="p">(){</span><span class="nx">$</span><span class="p">(</span><span class="k">this</span><span class="p">).</span><span class="nx">addClass</span><span class="p">(</span><span class="dl">'</span><span class="s1">bg</span><span class="dl">'</span><span class="p">);},</span> 
