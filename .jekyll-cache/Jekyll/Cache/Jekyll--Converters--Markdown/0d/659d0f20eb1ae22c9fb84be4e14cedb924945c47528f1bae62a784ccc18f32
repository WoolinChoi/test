<h2 id="popup_min">Popup_min</h2>
<div class="language-html highlighter-rouge"><div class="highlight"><pre class="highlight"><code><span class="cp">&lt;!DOCTYPE html&gt;</span>
<span class="nt">&lt;html&gt;</span>
<span class="nt">&lt;head&gt;</span>
<span class="nt">&lt;meta</span> <span class="na">charset=</span><span class="s">"UTF-8"</span><span class="nt">&gt;</span>
<span class="nt">&lt;title&gt;</span>ë©”ì¸í˜ì´ì§€<span class="nt">&lt;/title&gt;</span>
<span class="nt">&lt;script </span><span class="na">type=</span><span class="s">"text/javascript"</span> <span class="na">src=</span><span class="s">"http://code.jquery.com/jquery-3.4.1.js"</span><span class="nt">&gt;&lt;/script&gt;</span>
<span class="nt">&lt;script </span><span class="na">type=</span><span class="s">"text/javascript"</span> <span class="na">src=</span><span class="s">"js/jquery.cookie.js"</span><span class="nt">&gt;&lt;/script&gt;</span>
<span class="nt">&lt;script </span><span class="na">type=</span><span class="s">"text/javascript"</span><span class="nt">&gt;</span>
  <span class="c1">// ì§€ì •ëœ ì¿ í‚¤ ê°’ì„ ì–»ì–´ì˜¤ê¸° - í•´ë‹¹ê°’ì´ ì—†ì„ ë•Œë§Œ ì°½ ë„ìš°ê¸°</span>
  <span class="nx">$</span><span class="p">(</span><span class="kd">function</span><span class="p">(){</span>
    <span class="c1">// ì €ì¥ëœ ì¿ í‚¤ ê°’ì´ Yê°€ ì•„ë‹ˆë¼ë©´ íŒì—…ì°½ì„ ìƒì„±</span>
    <span class="k">if</span><span class="p">(</span><span class="nx">$</span><span class="p">.</span><span class="nx">cookie</span><span class="p">(</span><span class="dl">'</span><span class="s1">noPopup</span><span class="dl">'</span><span class="p">)</span> <span class="o">!=</span> <span class="dl">'</span><span class="s1">Y</span><span class="dl">'</span><span class="p">){</span>
      <span class="nb">window</span><span class="p">.</span><span class="nx">open</span><span class="p">(</span><span class="dl">"</span><span class="s2">9_promotion.html</span><span class="dl">"</span><span class="p">,</span><span class="dl">""</span><span class="p">,</span><span class="dl">"</span><span class="s2">width=400, height=360</span><span class="dl">"</span><span class="p">);</span>
    <span class="p">}</span>
    
    <span class="c1">// ì €ì¥ëœ ì¿ í‚¤ 'nopopup'ê°’ì„ 'Y'ë¡œ ë³€ê²½í•˜ê³  í˜ì´ì§€ë¥¼ ë¦¬ë¡œë“œ</span>
    <span class="nx">$</span><span class="p">(</span><span class="dl">'</span><span class="s1">#clear_option</span><span class="dl">'</span><span class="p">).</span><span class="nx">click</span><span class="p">(</span><span class="kd">function</span><span class="p">(){</span>
      <span class="c1">// ì¿ í‚¤ ê°’ì„ 'N'ìœ¼ë¡œ ë³€ê²½</span>
      <span class="nx">$</span><span class="p">.</span><span class="nx">cookie</span><span class="p">(</span><span class="dl">'</span><span class="s1">noPopup</span><span class="dl">'</span><span class="p">,</span> <span class="dl">'</span><span class="s1">N</span><span class="dl">'</span><span class="p">);</span>
      
      <span class="c1">// í˜ì´ì§€ ë¦¬ë¡œë“œ</span>
      <span class="nb">window</span><span class="p">.</span><span class="nx">location</span><span class="p">.</span><span class="nx">reload</span><span class="p">();</span>
    <span class="p">});</span>
  <span class="p">});</span>
<span class="nt">&lt;/script&gt;</span>
<span class="nt">&lt;/head&gt;</span>
