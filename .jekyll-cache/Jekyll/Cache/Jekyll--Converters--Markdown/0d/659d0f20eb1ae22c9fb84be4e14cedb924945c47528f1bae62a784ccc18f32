  <span class="nt">&lt;input</span> <span class="na">id=</span><span class="s">"txtMail"</span> <span class="na">type=</span><span class="s">"email"</span> <span class="na">size=</span><span class="s">"50"</span> <span class="nt">/&gt;</span>
