<span class="nt">&lt;table</span> <span class="na">width=</span><span class="s">800</span> <span class="na">cellpadding=</span><span class="s">0</span> <span class="na">cellspacing=</span><span class="s">0</span> <span class="na">border=</span><span class="s">0</span><span class="nt">&gt;</span>
	<span class="nt">&lt;tr&gt;</span>
