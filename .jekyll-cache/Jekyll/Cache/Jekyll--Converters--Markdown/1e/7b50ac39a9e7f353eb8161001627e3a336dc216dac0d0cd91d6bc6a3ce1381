    <span class="n">data</span> <span class="o">=</span> <span class="p">[</span><span class="n">row</span> <span class="k">for</span> <span class="n">row</span> <span class="ow">in</span> <span class="n">cin</span> <span class="k">if</span> <span class="n">row</span><span class="p">]</span>    <span class="c1"># rowê°€ ìˆìœ¼ë©´ ì¶œë ¥(ê³µë°±ì œê±°)
