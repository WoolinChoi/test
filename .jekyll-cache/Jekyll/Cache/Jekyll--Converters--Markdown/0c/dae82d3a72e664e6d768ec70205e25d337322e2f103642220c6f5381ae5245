    <span class="n">fruit</span><span class="o">.</span><span class="n">append</span><span class="p">(</span><span class="s">"e"</span><span class="p">)</span>
    <span class="n">fruit</span> <span class="o">=</span> <span class="p">[</span><span class="s">"apple"</span><span class="p">]</span>
    <span class="k">return</span> <span class="n">fruit</span>
