	<span class="o">}</span>	
	<span class="kd">public</span> <span class="kt">void</span> <span class="nf">output</span><span class="o">(</span><span class="kt">int</span> <span class="n">sel2</span><span class="o">)</span> <span class="o">{</span>
		<span class="kt">int</span> <span class="n">i</span> <span class="o">=</span> <span class="n">sel2</span><span class="o">;</span>
