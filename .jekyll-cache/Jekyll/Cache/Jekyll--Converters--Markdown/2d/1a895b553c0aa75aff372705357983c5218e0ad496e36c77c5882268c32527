		<span class="k">case</span> <span class="mi">1</span> <span class="o">:</span> <span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="n">greeting</span><span class="o">);</span> <span class="k">break</span><span class="o">;</span>
