<span class="k">INSERT</span> <span class="k">INTO</span> <span class="n">info</span><span class="p">(</span><span class="n">hakbun</span><span class="p">,</span> <span class="n">name</span><span class="p">,</span> <span class="n">gender</span><span class="p">)</span> <span class="k">VALUES</span> <span class="p">(</span><span class="s1">'1008'</span><span class="p">,</span> <span class="s1">'ì•ˆê¸¸ë™'</span><span class="p">,</span> <span class="s1">'ë‚¨ì'</span><span class="p">);</span> <span class="c1">-- CK ì—ëŸ¬</span>

<span class="c1">-- [DEFAULT] : ì œì•½ì¡°ê±´ì€ ì•„ë‹ˆì§€ë§Œ ì œì•½ì¡°ê±´ê³¼ ê°™ì´ ë§ì´ ì‚¬ìš©</span>
<span class="k">ALTER</span> <span class="k">TABLE</span> <span class="n">info</span> <span class="k">MODIFY</span> <span class="p">(</span><span class="n">gender</span> <span class="nb">char</span><span class="p">(</span><span class="mi">4</span><span class="p">)</span> <span class="k">DEFAULT</span> <span class="s1">'ë‚¨ì„±'</span><span class="p">);</span>
<span class="k">INSERT</span> <span class="k">INTO</span> <span class="n">info</span><span class="p">(</span><span class="n">hakbun</span><span class="p">,</span> <span class="n">name</span><span class="p">)</span> <span class="k">VALUES</span> <span class="p">(</span><span class="s1">'1009'</span><span class="p">,</span> <span class="s1">'ê°•ê¸¸ë™'</span><span class="p">);</span> <span class="c1">-- DEFAULTëŠ” ê¸°ë³¸ê°’ì„ ì§€ì •</span>

