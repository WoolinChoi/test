<h2 id="membervo">MemberVO</h2>
<div class="language-java highlighter-rouge"><div class="highlight"><pre class="highlight"><code><span class="kn">package</span> <span class="nn">com.javassem.domain</span><span class="o">;</span>

<span class="kd">public</span> <span class="kd">class</span> <span class="nc">MemberVO</span> <span class="o">{</span>
	<span class="kd">private</span> <span class="nc">String</span> <span class="n">userId</span><span class="o">;</span>
