<h2 id="videomodel">VideoModel</h2>
<div class="language-java highlighter-rouge"><div class="highlight"><pre class="highlight"><code><span class="kn">import</span> <span class="nn">java.sql.*</span><span class="o">;</span>
<span class="kn">import</span> <span class="nn">java.util.ArrayList</span><span class="o">;</span>
<span class="kn">import</span> <span class="nn">model.dao.VideoDao</span><span class="o">;</span>
<span class="kn">import</span> <span class="nn">model.vo.Video</span><span class="o">;</span>
