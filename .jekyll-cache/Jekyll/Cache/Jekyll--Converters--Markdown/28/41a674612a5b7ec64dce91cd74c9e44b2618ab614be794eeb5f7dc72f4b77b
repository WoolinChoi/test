<span class="kn">import</span> <span class="nn">model.vo.Customer</span><span class="o">;</span>
<span class="kd">public</span> <span class="kd">class</span> <span class="nc">CustomerView</span> <span class="kd">extends</span> <span class="nc">JPanel</span> <span class="o">{</span>
	<span class="nc">JTextField</span>	<span class="n">tfCustName</span><span class="o">,</span> <span class="n">tfCustTel</span><span class="o">,</span>  <span class="n">tfCustTelAid</span><span class="o">,</span> <span class="n">tfCustAddr</span><span class="o">,</span> <span class="n">tfCustEmail</span><span class="o">;</span>
	<span class="nc">JButton</span>		<span class="n">bCustRegist</span><span class="o">,</span> <span class="n">bCustModify</span><span class="o">;</span>
	
	<span class="nc">JTextField</span>  <span class="n">tfCustNameSearch</span><span class="o">,</span>  <span class="n">tfCustTelSearch</span><span class="o">;</span>
	<span class="nc">JButton</span>		<span class="n">bCustNameSearch</span><span class="o">,</span>  <span class="n">bCustTelSearch</span><span class="o">;</span>
	
	<span class="c1">// ë³€ìˆ˜</span>
	<span class="nc">CustomerDao</span> <span class="n">dao</span><span class="o">;</span>
	
	<span class="kd">public</span> <span class="nf">CustomerView</span><span class="o">()</span> <span class="o">{</span>
		<span class="n">addLayout</span><span class="o">();</span>
		<span class="n">connectDB</span><span class="o">();</span>
		<span class="n">eventProc</span><span class="o">();</span>
	<span class="o">}</span>
	
	<span class="kd">public</span> <span class="kt">void</span> <span class="nf">eventProc</span><span class="o">()</span> <span class="o">{</span>
		<span class="nc">ButtonEventHandler</span> <span class="n">btnHandler</span> <span class="o">=</span> <span class="k">new</span> <span class="nc">ButtonEventHandler</span><span class="o">();</span>
		
		<span class="c1">// ì´ë²¤íŠ¸ ë“±ë¡</span>
		<span class="n">bCustRegist</span><span class="o">.</span><span class="na">addActionListener</span><span class="o">(</span><span class="n">btnHandler</span><span class="o">);</span>
		<span class="n">bCustModify</span><span class="o">.</span><span class="na">addActionListener</span><span class="o">(</span><span class="n">btnHandler</span><span class="o">);</span>
		<span class="n">bCustNameSearch</span><span class="o">.</span><span class="na">addActionListener</span><span class="o">(</span><span class="n">btnHandler</span><span class="o">);</span>
		<span class="n">bCustTelSearch</span><span class="o">.</span><span class="na">addActionListener</span><span class="o">(</span><span class="n">btnHandler</span><span class="o">);</span>
	<span class="o">}</span>
	
	<span class="c1">// ë²„íŠ¼ ì´ë²¤íŠ¸ í•¸ë“¤ëŸ¬ ë§Œë“¤ê¸°</span>
	<span class="kd">class</span> <span class="nc">ButtonEventHandler</span> <span class="kd">implements</span> <span class="nc">ActionListener</span> <span class="o">{</span>
		<span class="kd">public</span> <span class="kt">void</span> <span class="nf">actionPerformed</span><span class="o">(</span><span class="nc">ActionEvent</span> <span class="n">ev</span><span class="o">)</span> <span class="o">{</span>
			<span class="nc">Object</span> <span class="n">o</span> <span class="o">=</span> <span class="n">ev</span><span class="o">.</span><span class="na">getSource</span><span class="o">();</span>
			
			<span class="k">if</span><span class="o">(</span><span class="n">o</span> <span class="o">==</span> <span class="n">bCustRegist</span><span class="o">)</span> <span class="o">{</span>  
				<span class="n">registCustomer</span><span class="o">();</span>  <span class="c1">// íšŒì›ë“±ë¡</span>
			<span class="o">}</span>
			<span class="k">else</span> <span class="nf">if</span><span class="o">(</span><span class="n">o</span> <span class="o">==</span> <span class="n">bCustModify</span><span class="o">)</span> <span class="o">{</span>  
				<span class="n">updateCustomer</span><span class="o">();</span>  <span class="c1">// íšŒì›ì •ë³´ìˆ˜ì •</span>
			<span class="o">}</span>			
			<span class="k">else</span>  <span class="nf">if</span><span class="o">(</span><span class="n">o</span> <span class="o">==</span> <span class="n">bCustTelSearch</span><span class="o">)</span> <span class="o">{</span> 
				<span class="n">searchByTel</span><span class="o">();</span>      <span class="c1">// ì „í™”ë²ˆí˜¸ ê²€ìƒ‰</span>
			<span class="o">}</span>
			<span class="k">else</span> <span class="nf">if</span><span class="o">(</span><span class="n">o</span> <span class="o">==</span> <span class="n">bCustNameSearch</span><span class="o">)</span> <span class="o">{</span>  <span class="c1">// ì´ë¦„ê²€ìƒ‰</span>
				<span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="s">"ì´ë¦„ê²€ìƒ‰"</span><span class="o">);</span>
			<span class="o">}</span>
		<span class="o">}</span>
	<span class="o">}</span>
	
	<span class="c1">// íšŒì›ê°€ì…í•˜ëŠ” ë©”ì†Œë“œ</span>
	<span class="kd">public</span> <span class="kt">void</span> <span class="nf">registCustomer</span><span class="o">()</span> <span class="o">{</span>
		<span class="c1">// 1. í™”ë©´ í…ìŠ¤íŠ¸í•„ë“œì˜ ì…ë ¥ê°’ ì–»ì–´ì˜¤ê¸°			</span>
		<span class="c1">// 2. 1ê°’ë“¤ì„ Customer í´ë˜ìŠ¤ì˜ ë©¤ë²„ë¡œì§€ì •			</span>
		<span class="c1">// 3. Model í´ë˜ìŠ¤ ì•ˆì— insertCustomer () ë©”ì†Œë“œ í˜¸ì¶œí•˜ì—¬ 2ë²ˆ VO ê°ì²´ë¥¼ ë„˜ê¹€</span>
		<span class="c1">// 4. í™”ë©´ ì´ˆê¸°í™”</span>
		<span class="nc">Customer</span> <span class="n">vo</span> <span class="o">=</span> <span class="k">new</span> <span class="nc">Customer</span><span class="o">();</span>
		<span class="n">vo</span><span class="o">.</span><span class="na">setCustTel1</span><span class="o">(</span><span class="n">tfCustName</span><span class="o">.</span><span class="na">getText</span><span class="o">());</span>
		<span class="n">vo</span><span class="o">.</span><span class="na">setCustName</span><span class="o">(</span><span class="n">tfCustTel</span><span class="o">.</span><span class="na">getText</span><span class="o">());</span>
		<span class="n">vo</span><span class="o">.</span><span class="na">setCustTel2</span><span class="o">(</span><span class="n">tfCustTelAid</span><span class="o">.</span><span class="na">getText</span><span class="o">());</span>
