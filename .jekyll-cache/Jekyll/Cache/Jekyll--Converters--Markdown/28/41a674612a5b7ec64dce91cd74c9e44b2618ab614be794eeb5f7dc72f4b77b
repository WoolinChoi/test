		<span class="n">cbc</span><span class="o">.</span><span class="na">gridwidth</span> <span class="o">=</span>	<span class="mi">1</span><span class="o">;</span>			
		<span class="n">cbc</span><span class="o">.</span><span class="na">gridheight</span> <span class="o">=</span> <span class="mi">1</span><span class="o">;</span>
		<span class="n">pRegist</span><span class="o">.</span><span class="na">add</span><span class="o">(</span><span class="k">new</span> <span class="nc">JLabel</span><span class="o">(</span><span class="s">" ì´ë©”ì¼ "</span><span class="o">),</span> <span class="n">cbc</span><span class="o">);</span>
		
		<span class="n">cbc</span><span class="o">.</span><span class="na">gridx</span> <span class="o">=</span>	<span class="mi">1</span><span class="o">;</span>	 			
