				<span class="nc">ArrayList</span> <span class="n">temp</span> <span class="o">=</span> <span class="o">(</span><span class="nc">ArrayList</span><span class="o">)</span><span class="n">data</span><span class="o">.</span><span class="na">get</span><span class="o">(</span><span class="n">row</span><span class="o">);</span>
