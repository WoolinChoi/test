		<span class="n">vo</span><span class="o">.</span><span class="na">setExp</span><span class="o">(</span><span class="n">taVideoContent</span><span class="o">.</span><span class="na">getText</span><span class="o">());</span>
		<span class="kt">int</span> <span class="n">count</span> <span class="o">=</span> <span class="nc">Integer</span><span class="o">.</span><span class="na">valueOf</span><span class="o">(</span><span class="n">tfInsertCount</span><span class="o">.</span><span class="na">getText</span><span class="o">());</span>

