		<span class="n">rentTM</span> <span class="o">=</span> <span class="k">new</span> <span class="nc">RentTableModel</span><span class="o">();</span>
		<span class="n">tableRecentList</span> <span class="o">=</span> <span class="k">new</span> <span class="nc">JTable</span><span class="o">(</span><span class="n">rentTM</span><span class="o">);</span>
		
		<span class="c1">// í™”ë©´ êµ¬ì„±</span>
		<span class="c1">// ìœ„ìª½ ì˜ì—­</span>
