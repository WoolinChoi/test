		<span class="n">pSearch</span><span class="o">.</span><span class="na">add</span><span class="o">(</span><span class="n">pSearchTel</span><span class="o">);</span>

		<span class="c1">// ì „ì²´ íŒ¨ë„ì— ë¶™ì´ê¸°</span>
		<span class="n">setLayout</span><span class="o">(</span><span class="k">new</span> <span class="nc">BorderLayout</span><span class="o">());</span>
		<span class="n">add</span><span class="o">(</span><span class="s">"Center"</span><span class="o">,</span> <span class="n">pRegist</span><span class="o">);</span>
		<span class="n">add</span><span class="o">(</span><span class="s">"South"</span><span class="o">,</span> <span class="n">pSearch</span><span class="o">);</span>	
	<span class="o">}</span>
<span class="o">}</span>		
