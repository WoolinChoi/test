				<span class="n">pSearchTel</span><span class="o">.</span><span class="na">add</span><span class="o">(</span><span class="k">new</span> <span class="nc">JLabel</span><span class="o">(</span><span class="s">" ì „í™”ë²ˆí˜¸ "</span><span class="o">));</span>
				<span class="n">pSearchTel</span><span class="o">.</span><span class="na">add</span><span class="o">(</span><span class="n">tfCustTelSearch</span><span class="o">);</span>
