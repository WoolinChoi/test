		    <span class="o">}</span>
		    
		    <span class="kd">public</span> <span class="nc">String</span> <span class="nf">getColumnName</span><span class="o">(</span><span class="kt">int</span> <span class="n">col</span><span class="o">)</span> <span class="o">{</span>
		    	<span class="k">return</span> <span class="n">columnNames</span><span class="o">[</span><span class="n">col</span><span class="o">];</span>
		    <span class="o">}</span>
	<span class="o">}</span>
<span class="o">}</span>
