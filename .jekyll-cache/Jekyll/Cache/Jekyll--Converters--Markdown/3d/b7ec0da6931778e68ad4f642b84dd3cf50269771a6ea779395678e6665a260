<h2 id="priority">Priority</h2>
<div class="language-java highlighter-rouge"><div class="highlight"><pre class="highlight"><code><span class="kd">class</span> <span class="nc">MakeCar</span> <span class="kd">extends</span> <span class="nc">Thread</span> <span class="o">{</span>
	<span class="nc">String</span> <span class="n">work</span><span class="o">;</span>
	<span class="nc">MakeCar</span><span class="o">(</span><span class="nc">String</span> <span class="n">_work</span><span class="o">,</span> <span class="kt">int</span> <span class="n">prio</span><span class="o">)</span> <span class="o">{</span>
		<span class="n">work</span> <span class="o">=</span> <span class="n">_work</span><span class="o">;</span>
		<span class="n">setPriority</span><span class="o">(</span><span class="n">prio</span><span class="o">);</span>
	<span class="o">}</span>
	<span class="kd">public</span> <span class="kt">void</span> <span class="nf">run</span><span class="o">()</span> <span class="o">{</span>
		<span class="k">for</span><span class="o">(</span><span class="kt">int</span> <span class="n">i</span> <span class="o">=</span> <span class="mi">0</span><span class="o">;</span> <span class="n">i</span> <span class="o">&lt;</span> <span class="mi">5</span><span class="o">;</span> <span class="n">i</span><span class="o">++)</span> <span class="o">{</span>
			<span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="n">work</span> <span class="o">+</span> <span class="s">"ì‘ì—…ì¤‘"</span><span class="o">);</span>
			<span class="k">try</span> <span class="o">{</span>
				<span class="nc">Thread</span><span class="o">.</span><span class="na">sleep</span><span class="o">(</span><span class="mi">500</span><span class="o">);</span>
			<span class="o">}</span><span class="k">catch</span><span class="o">(</span><span class="nc">InterruptedException</span> <span class="n">e</span><span class="o">)</span> <span class="o">{</span>
				<span class="n">e</span><span class="o">.</span><span class="na">printStackTrace</span><span class="o">();</span>
			<span class="o">}</span>
		<span class="o">}</span>
	<span class="o">}</span>
<span class="o">}</span>
<span class="kd">public</span> <span class="kd">class</span> <span class="nc">Ex06_Priority</span> <span class="o">{</span>
	<span class="kd">public</span> <span class="kd">static</span> <span class="kt">void</span> <span class="nf">main</span><span class="o">(</span><span class="nc">String</span><span class="o">[]</span> <span class="n">args</span><span class="o">)</span> <span class="o">{</span>
		<span class="nc">MakeCar</span> <span class="n">mc1</span> <span class="o">=</span> <span class="k">new</span> <span class="nc">MakeCar</span><span class="o">(</span><span class="s">"ì°¨í‹€"</span><span class="o">,</span> <span class="nc">Thread</span><span class="o">.</span><span class="na">NORM_PRIORITY</span><span class="o">);</span>
		<span class="c1">// ìš°ì„ ìˆœìœ„ëŠ” í° ì˜í–¥ì´ ì—†ë‹¤.</span>
		<span class="n">mc1</span><span class="o">.</span><span class="na">start</span><span class="o">();</span>
		<span class="nc">MakeCar</span> <span class="n">mc2</span> <span class="o">=</span> <span class="k">new</span> <span class="nc">MakeCar</span><span class="o">(</span><span class="s">"ë„ìƒ‰"</span><span class="o">,</span> <span class="nc">Thread</span><span class="o">.</span><span class="na">NORM_PRIORITY</span><span class="o">);</span>
		<span class="n">mc2</span><span class="o">.</span><span class="na">start</span><span class="o">();</span>
	<span class="o">}</span>
<span class="o">}</span>
</code></pre></div></div>

<h2 id="synch">Synch</h2>
<div class="language-java highlighter-rouge"><div class="highlight"><pre class="highlight"><code><span class="kd">class</span> <span class="nc">Count</span> <span class="o">{</span>
	<span class="kt">int</span> <span class="n">i</span><span class="o">;</span>
	<span class="c1">// Javaì—ì„œ lockì€ synchronizedì´ë‹¤.</span>
	<span class="kt">void</span> <span class="nf">increment</span><span class="o">()</span> <span class="o">{</span>
		<span class="c1">// ë©”ì†Œë“œì— ë“¤ì–´ê°€ëŠ”ê±° ì•„ë‹ˆë©´ ë°˜ë“œì‹œ ê³µìœ í•˜ëŠ” ê°ì²´ í•˜ë‚˜(thisë¼ë„)ë¥¼ ê°€ì§€ê³ ë“¤ì–´ê°„ë‹¤. </span>
		<span class="kd">synchronized</span><span class="o">(</span><span class="k">this</span><span class="o">)</span> <span class="o">{</span>
			<span class="n">i</span><span class="o">++;</span>
		<span class="o">}</span>
	<span class="o">}</span>
<span class="o">}</span>
<span class="kd">class</span> <span class="nc">ThreadCount</span> <span class="kd">extends</span> <span class="nc">Thread</span> <span class="o">{</span>
	<span class="nc">Count</span> <span class="n">cnt</span><span class="o">;</span>
	<span class="c1">// ìƒì„±ì í•¨ìˆ˜</span>
	<span class="kd">public</span> <span class="nf">ThreadCount</span><span class="o">(</span><span class="nc">Count</span> <span class="n">cnt</span><span class="o">)</span> <span class="o">{</span>
		<span class="k">this</span><span class="o">.</span><span class="na">cnt</span> <span class="o">=</span> <span class="n">cnt</span><span class="o">;</span>
	<span class="o">}</span>
	<span class="c1">// ìŠ¤ë ˆë“œëŠ” run ë©”ì†Œë“œë¥¼ ì˜¤ë²„ë¼ì´ë”© í•´ì¤˜ì•¼í•œë‹¤.</span>
	<span class="kd">public</span> <span class="kt">void</span> <span class="nf">run</span><span class="o">()</span> <span class="o">{</span>
		<span class="k">for</span><span class="o">(</span><span class="kt">int</span> <span class="n">i</span> <span class="o">=</span> <span class="mi">0</span><span class="o">;</span> <span class="n">i</span> <span class="o">&lt;</span> <span class="mi">100000000</span><span class="o">;</span> <span class="n">i</span><span class="o">++)</span> <span class="o">{</span>
			<span class="n">cnt</span><span class="o">.</span><span class="na">increment</span><span class="o">();</span>
		<span class="o">}</span>
	<span class="o">}</span>
<span class="o">}</span>
<span class="kd">public</span> <span class="kd">class</span> <span class="nc">Ex07_Synch</span> <span class="o">{</span>
	<span class="kd">public</span> <span class="kd">static</span> <span class="kt">void</span> <span class="nf">main</span><span class="o">(</span><span class="nc">String</span><span class="o">[]</span> <span class="n">args</span><span class="o">)</span> <span class="o">{</span>
		<span class="nc">Count</span> <span class="n">cnt</span> <span class="o">=</span> <span class="k">new</span> <span class="nc">Count</span><span class="o">();</span>
		<span class="nc">ThreadCount</span> <span class="n">tc1</span> <span class="o">=</span> <span class="k">new</span> <span class="nc">ThreadCount</span><span class="o">(</span><span class="n">cnt</span><span class="o">);</span>
		<span class="n">tc1</span><span class="o">.</span><span class="na">start</span><span class="o">();</span>
		<span class="nc">ThreadCount</span> <span class="n">tc2</span> <span class="o">=</span> <span class="k">new</span> <span class="nc">ThreadCount</span><span class="o">(</span><span class="n">cnt</span><span class="o">);</span>
		<span class="n">tc2</span><span class="o">.</span><span class="na">start</span><span class="o">();</span>
		<span class="k">try</span> <span class="o">{</span>
			<span class="n">tc1</span><span class="o">.</span><span class="na">join</span><span class="o">();</span>
			<span class="n">tc2</span><span class="o">.</span><span class="na">join</span><span class="o">();</span>
		<span class="o">}</span><span class="k">catch</span><span class="o">(</span><span class="nc">Exception</span> <span class="n">ex</span><span class="o">)</span> <span class="o">{}</span>
		<span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="s">"iê°’: "</span> <span class="o">+</span> <span class="n">cnt</span><span class="o">.</span><span class="na">i</span><span class="o">);</span>
	<span class="o">}</span>
<span class="o">}</span>
</code></pre></div></div>

<h2 id="breadtest">BreadTest</h2>
<div class="language-java highlighter-rouge"><div class="highlight"><pre class="highlight"><code><span class="kd">class</span> <span class="nc">Bread</span> <span class="o">{</span>
	<span class="nc">String</span> <span class="n">bread</span><span class="o">;</span>
	<span class="kt">boolean</span> <span class="n">isCheck</span> <span class="o">=</span> <span class="kc">false</span><span class="o">;</span>
	<span class="c1">// wait()/notigyAll()  	</span>
	<span class="kd">public</span> <span class="kt">void</span> <span class="nf">setBread</span><span class="o">(</span><span class="nc">String</span> <span class="n">bread</span><span class="o">)</span> <span class="o">{</span>
		<span class="k">this</span><span class="o">.</span><span class="na">bread</span> <span class="o">=</span> <span class="n">bread</span><span class="o">;</span>		
		<span class="n">isCheck</span> <span class="o">=</span> <span class="kc">true</span><span class="o">;</span>
		<span class="kd">synchronized</span><span class="o">(</span><span class="n">bread</span><span class="o">)</span> <span class="o">{</span>
			<span class="n">notifyAll</span><span class="o">();</span>	
		<span class="o">}</span>
	<span class="o">}</span>	
	<span class="kd">public</span> <span class="nc">String</span> <span class="nf">getBread</span><span class="o">()</span> <span class="o">{</span>
		<span class="k">if</span><span class="o">(</span><span class="n">isCheck</span> <span class="o">==</span> <span class="kc">false</span><span class="o">)</span> <span class="o">{</span>
				<span class="k">try</span> <span class="o">{</span>
					<span class="kd">synchronized</span><span class="o">(</span><span class="n">bread</span><span class="o">)</span> <span class="o">{</span>
						<span class="n">wait</span><span class="o">();</span>
					<span class="o">}</span>
				<span class="o">}</span> <span class="k">catch</span> <span class="o">(</span><span class="nc">InterruptedException</span> <span class="n">e</span><span class="o">)</span> <span class="o">{</span>
					<span class="n">e</span><span class="o">.</span><span class="na">printStackTrace</span><span class="o">();</span>
				<span class="o">}</span>
		<span class="o">}</span>
		<span class="k">return</span> <span class="n">bread</span><span class="o">;</span>
	<span class="o">}</span>
<span class="o">}</span>
