		<span class="k">this</span><span class="o">.</span><span class="na">bbang</span> <span class="o">=</span> <span class="n">bbang</span><span class="o">;</span>
	<span class="o">}</span>
	<span class="kd">public</span> <span class="kt">void</span> <span class="nf">run</span><span class="o">()</span> <span class="o">{</span>
		<span class="n">bbang</span><span class="o">.</span><span class="na">setBread</span><span class="o">(</span><span class="s">"ì§„ì—´ëœ ì™„ì„±ëœ ë§›ìˆëŠ” ë¹µ"</span><span class="o">);</span>
	<span class="o">}</span>
<span class="o">}</span>
<span class="kd">class</span> <span class="nc">Customer</span> <span class="kd">extends</span> <span class="nc">Thread</span> <span class="o">{</span>
	<span class="nc">Bread</span> <span class="n">bbang</span><span class="o">;</span>
	<span class="nc">Customer</span><span class="o">(</span><span class="nc">Bread</span> <span class="n">bbang</span><span class="o">)</span> <span class="o">{</span>
		<span class="k">this</span><span class="o">.</span><span class="na">bbang</span> <span class="o">=</span> <span class="n">bbang</span><span class="o">;</span>
	<span class="o">}</span>
	<span class="kd">public</span> <span class="kt">void</span> <span class="nf">run</span><span class="o">()</span> <span class="o">{</span>
		<span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">.</span><span class="na">println</span><span class="o">(</span><span class="s">"ë¹µì„ ì‚¬ê° : "</span> <span class="o">+</span> <span class="n">bbang</span><span class="o">.</span><span class="na">getBread</span><span class="o">());</span>
	<span class="o">}</span>
<span class="o">}</span>
<span class="kd">class</span> <span class="nc">Ex08_BreadTest</span> <span class="o">{</span>
	<span class="kd">public</span> <span class="kd">static</span> <span class="kt">void</span> <span class="nf">main</span><span class="o">(</span><span class="nc">String</span><span class="o">[]</span> <span class="n">args</span><span class="o">)</span> <span class="o">{</span>
