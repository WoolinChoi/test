    <span class="kd">private</span> <span class="kt">boolean</span> <span class="n">inputChk</span><span class="o">;</span>
    <span class="kd">public</span> <span class="nf">Ex04_CounterTest</span><span class="o">()</span> <span class="o">{</span>
        <span class="n">setTitle</span><span class="o">(</span><span class="s">"ë‹¨ì¼ ìŠ¤ë ˆë“œ í…ŒìŠ¤íŠ¸!"</span><span class="o">);</span>
        <span class="n">p1</span> <span class="o">=</span> <span class="k">new</span> <span class="nc">JPanel</span><span class="o">();</span>
        <span class="n">p1</span><span class="o">.</span><span class="na">add</span><span class="o">(</span><span class="n">btn</span> <span class="o">=</span> <span class="k">new</span> <span class="nc">JButton</span><span class="o">(</span><span class="s">"Click"</span><span class="o">));</span>
        <span class="n">p1</span><span class="o">.</span><span class="na">add</span><span class="o">(</span><span class="n">lb</span> <span class="o">=</span> <span class="k">new</span> <span class="nc">JLabel</span><span class="o">(</span><span class="s">"Count!"</span><span class="o">));</span> <span class="c1">// ì¶”ê°€ </span>
        <span class="n">add</span><span class="o">(</span><span class="n">p1</span><span class="o">,</span><span class="s">"North"</span><span class="o">);</span>
        <span class="n">p2</span> <span class="o">=</span> <span class="k">new</span> <span class="nc">JPanel</span><span class="o">();</span>       
        <span class="n">p2</span><span class="o">.</span><span class="na">add</span><span class="o">(</span> <span class="n">ta</span> <span class="o">=</span> <span class="k">new</span> <span class="nc">JTextArea</span><span class="o">(</span><span class="mi">20</span><span class="o">,</span><span class="mi">50</span><span class="o">));</span>
