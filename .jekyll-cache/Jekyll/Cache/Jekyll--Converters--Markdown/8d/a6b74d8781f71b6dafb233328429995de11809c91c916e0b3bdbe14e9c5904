<h2 id="regexp">Regexp</h2>
<div class="language-html highlighter-rouge"><div class="highlight"><pre class="highlight"><code><span class="cp">&lt;!DOCTYPE html&gt;</span>
<span class="nt">&lt;html&gt;</span>
<span class="nt">&lt;head&gt;</span>
<span class="nt">&lt;meta</span> <span class="na">charset=</span><span class="s">"UTF-8"</span><span class="nt">&gt;</span>
<span class="nt">&lt;title&gt;</span>Regexp<span class="nt">&lt;/title&gt;</span>
<span class="nt">&lt;script </span><span class="na">type=</span><span class="s">"text/javascript"</span><span class="nt">&gt;</span>
	<span class="kd">var</span> <span class="nx">p</span> <span class="o">=</span> <span class="sr">/http</span><span class="se">(</span><span class="sr">s</span><span class="se">)?</span><span class="sr">:</span><span class="se">\/\/([\w</span><span class="sr">-</span><span class="se">]</span><span class="sr">+</span><span class="se">\.)</span><span class="sr">+</span><span class="se">[\w</span><span class="sr">-</span><span class="se">]</span><span class="sr">+</span><span class="se">(\/[\w</span><span class="sr">-.</span><span class="se">\/</span><span class="sr">?%&amp;=</span><span class="se">]</span><span class="sr">*</span><span class="se">)?</span><span class="sr">/gi</span><span class="p">;</span>
	<span class="c1">//var p = new RegExp('http(s)?://([\\w-]+\\.)+[\\w-]+(/[\\w-./?%&amp;=]*)?','gi');</span>
	<span class="kd">var</span> <span class="nx">str</span> <span class="o">=</span> <span class="dl">'</span><span class="s1">í•œ ë²ˆì„ ë‘˜ëŸ¬ë´ì•¼ í•˜ëŠ” ì‚¬ì´íŠ¸ http://www.w3schools.com/ ì…ë‹ˆë‹¤</span><span class="dl">'</span><span class="p">;</span>
	<span class="nx">str</span> <span class="o">+=</span> <span class="dl">'</span><span class="s1"> ë˜í•œ ì§‘ì¤‘ì ìœ¼ë¡œ ë´ì•¼í•˜ëŠ” ì‚¬ì´íŠ¸ http://jqapi.ru/ ì…ë‹ˆë‹¤</span><span class="dl">'</span><span class="p">;</span>
	
	<span class="c1">// 1. match() : ì •ê·œì‹ ê²°ê³¼ë¥¼ ë³´ì—¬ì£¼ëŠ” í•¨ìˆ˜</span>
	<span class="kd">var</span> <span class="nx">result</span> <span class="o">=</span> <span class="nx">str</span><span class="p">.</span><span class="nx">match</span><span class="p">(</span><span class="nx">p</span><span class="p">);</span>
	<span class="k">for</span><span class="p">(</span><span class="kd">var</span> <span class="nx">i</span> <span class="o">=</span> <span class="mi">0</span><span class="p">;</span> <span class="nx">i</span> <span class="o">&lt;</span> <span class="nx">result</span><span class="p">.</span><span class="nx">length</span><span class="p">;</span> <span class="nx">i</span><span class="o">++</span><span class="p">){</span>
		<span class="nb">document</span><span class="p">.</span><span class="nx">write</span><span class="p">(</span><span class="nx">result</span><span class="p">[</span><span class="nx">i</span><span class="p">]</span> <span class="o">+</span> <span class="dl">'</span><span class="s1">&lt;br/&gt;</span><span class="dl">'</span><span class="p">);</span>
	<span class="p">}</span>
	
	<span class="c1">// 2. exec() - ê²°ê³¼ê°€ ì´ìƒí•¨</span>
	<span class="kd">var</span> <span class="nx">result2</span><span class="p">;</span>
	<span class="k">while</span><span class="p">((</span><span class="nx">result2</span> <span class="o">=</span> <span class="nx">p</span><span class="p">.</span><span class="nx">exec</span><span class="p">(</span><span class="nx">str</span><span class="p">))</span> <span class="o">!=</span> <span class="kc">null</span><span class="p">){</span>
		<span class="nb">document</span><span class="p">.</span><span class="nx">write</span><span class="p">(</span><span class="nx">result2</span> <span class="o">+</span> <span class="dl">'</span><span class="s1">&lt;br/&gt;</span><span class="dl">'</span><span class="p">);</span>
	<span class="p">}</span>
	
	<span class="c1">// 3. test() : ì—¬ëŸ¬ ì •ê·œì‹ì—ì„œ ë§ëŠ” ë¬¸ì¥ì´ ë‚˜ì˜¤ë©´ trueì´ê³  ë‚˜ë¨¸ì§€ëŠ” false  </span>
	<span class="kd">var</span> <span class="nx">one</span> <span class="o">=</span> <span class="dl">'</span><span class="s1">í•œ ë²ˆì„ ë‘˜ëŸ¬ë´ì•¼ í•˜ëŠ” ì‚¬ì´íŠ¸ http://www.w3schools.com/ ì…ë‹ˆë‹¤</span><span class="dl">'</span><span class="p">;</span>
	<span class="kd">var</span> <span class="nx">two</span> <span class="o">=</span> <span class="dl">'</span><span class="s1"> ë˜í•œ ì§‘ì¤‘ì ìœ¼ë¡œ ë´ì•¼í•˜ëŠ” ì‚¬ì´íŠ¸ http://jqapi.ru/ ì…ë‹ˆë‹¤</span><span class="dl">'</span><span class="p">;</span>
	<span class="nb">document</span><span class="p">.</span><span class="nx">write</span><span class="p">(</span><span class="nx">p</span><span class="p">.</span><span class="nx">test</span><span class="p">(</span><span class="nx">one</span><span class="p">)</span> <span class="o">+</span> <span class="dl">'</span><span class="s1">&lt;br/&gt;</span><span class="dl">'</span><span class="p">);</span> <span class="c1">// true</span>
	<span class="nb">document</span><span class="p">.</span><span class="nx">write</span><span class="p">(</span><span class="nx">p</span><span class="p">.</span><span class="nx">test</span><span class="p">(</span><span class="nx">two</span><span class="p">)</span> <span class="o">+</span> <span class="dl">'</span><span class="s1">&lt;br/&gt;</span><span class="dl">'</span><span class="p">);</span> <span class="c1">// false</span>
	
	<span class="c1">// search()</span>
	<span class="nb">document</span><span class="p">.</span><span class="nx">write</span><span class="p">(</span><span class="nx">one</span><span class="p">.</span><span class="nx">search</span><span class="p">(</span><span class="nx">p</span><span class="p">)</span> <span class="o">+</span> <span class="dl">'</span><span class="s1">&lt;br/&gt;</span><span class="dl">'</span><span class="p">);</span>
	<span class="nb">document</span><span class="p">.</span><span class="nx">write</span><span class="p">(</span><span class="nx">two</span><span class="p">.</span><span class="nx">search</span><span class="p">(</span><span class="nx">p</span><span class="p">)</span> <span class="o">+</span> <span class="dl">'</span><span class="s1">&lt;br/&gt;</span><span class="dl">'</span><span class="p">);</span>
